#!/usr/bin/env python3
import subprocess
import sys
import os
cwd = os.path.dirname(os.path.abspath(__file__))#Not having this line leads to error that it could not find appindicator.py
relpath = '../src/appindicator.py'
subprocess.Popen(["python3", relpath], cwd=cwd, stdin=subprocess.DEVNULL, stdout=subprocess.DEVNULL, stderr=subprocess.DEVNULL)
sys.exit(0)